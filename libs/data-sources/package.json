{
  "name": "@siafoundation/data-sources",
  "version": "0.4.0",
  "private": true,
  "description": "Data sources used for stats on the website.",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-p bundle type:gen",
    "build:dev": "run-p bundle:watch type:gen:watch",
    "bundle": "tsup src/index.ts --format esm",
    "clean": "del dist tsconfig.tsbuildinfo",
    "coverage": "vitest run --coverage ",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest",
    "type:check": "tsc --noEmit",
    "type:gen": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.8",
    "@technically/lodash": "^4.17.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "next-mdx-remote": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@siafoundation/typescript-config": "^0.0.0",
    "@swc/core": "^1.6.13",
    "del-cli": "^5.1.0",
    "tsup": "^8.1.0",
    "react": "18.3.0",
    "react-dom": "18.3.0"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "18.3.0"
  }
}
